   "source": [
    "def cost(t0, t1, list):\n",
    "    m = len(list)\n",
    "    sums = int(0)\n",
    "    for item in list:\n",
    "        x,y = item\n",
    "        ho = t0 + t1 * x\n",
    "        sums += (ho - y)**2\n",
    "    res = (1/(2*m))*sums\n",
    "    return res\n",
    "\n",
    "list = [(1,1),(2,2),(3,3)]\n",
    "t0 = 0\n",
    "t1 = 1\n",
    "error = cost(t0, t1, list)\n",
    "print(error)"
   ]
